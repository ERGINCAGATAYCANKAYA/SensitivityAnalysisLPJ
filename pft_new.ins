group "C3G" (

	include C3G_include! 1
	grass
	pathway C3G_pathway! "c3"
	rootdist 0.18 0.18 0.18 0.18 0.18 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01
	root_beta C3G_root_beta! 0.9555
	gmin C3G_gmin! 0.5
	phenology C3G_phenology! "any"
    sla 32.4				! Not used if ifcalcsla run_ifcalcsla! is set
	leaflong C3G_leaflong! 0.5				! MF recommended to give grasses competitive edge of higher SLA than deciduous trees
	turnover_leaf C3G_turnover_leaf! 1
	turnover_root C3G_turnover_root! 0.5		! THreturned (0.7)
	phengdd5ramp C3G_phengdd5ramp! 100
	pstemp_min C3G_pstemp_min! -5
	pstemp_low C3G_pstemp_low! 10
	pstemp_high C3G_pstemp_high! 30
	pstemp_max C3G_pstemp_max! 45
	tcmin_surv C3G_tcmin_surv! -1000		! limit not implemented
	tcmin_est C3G_tcmin_est! -1000			! limit not implemented
	tcmax_est C3G_tcmax_est! 1000			! limit not implemented
	twmin_est C3G_twmin_est! -1000			! limit not implemented
	gdd5min_est C3G_gdd5min_est! 0			! limit not implemented
