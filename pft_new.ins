searchradius run_searchradius! 0.25
outputdirectory run_outputdirectory! "./"
vegmode run_vegmode! "cohort"      ! "cohort", "individual" or "population"
nyear_spinup run_nyear_spinup! 500      ! number of years to spin up the simulation for    !500
ifcalcsla run_ifcalcsla! 0           ! whether to calculate SLA from leaf longevity
ifcalccton run_ifcalccton! 1          ! whether to calculate leaf C:N min from leaf longevity
iffire run_iffire! 1              ! whether to implement fire (1) or not (0)
patcharea run_patcharea! 1000        ! patch area (m2)
estinterval run_estinterval! 5         ! years between establishment events in cohort mode
ifdisturb run_ifdisturb! 1           ! whether generic patch-destroying disturbances enabled
distinterval run_distinterval! 920      ! average return time for generic patch-destroying disturbances
ifbgestab run_ifbgestab! 1           ! whether background establishment enabled
ifsme run_ifsme! 1               ! whether spatial mass effect enabled
ifstochestab run_ifstochestab! 1        ! whether establishment stochastic
ifstochmort run_ifstochmort! 1         ! whether mortality stochastic
ifcdebt run_ifcdebt! 1             ! whether to allow vegetation C storage (1) or not (0)
wateruptake run_wateruptake! "rootdist"! "wcont", "rootdist", "smart" or "speciesspecific",
rootdistribution run_rootdistribution! "jackson"	! How to parameterise root distribution. Alternatives are "fixed" or "jackson".
textured_soil run_textured_soil! 1	      ! whether to use silt/sand fractions specific to soiltype
ifcentury run_ifcentury! 1			! whether to use CENTURY SOM dynamics (mandatory for N cycling)
freenyears run_freenyears! 100			! number of years to spin up without N limitation (needed to build up a N pool)
nfix_a run_nfix_a! 0.102			! first term in N fixation eqn (Conservative 0.102, Central 0.234, Upper 0.367)
nfix_b run_nfix_b! 0.524			! second term in N fixation eqn (Conservative 0.524, Central -0.172, Upper -0.754)
nrelocfrac run_nrelocfrac! 0.5			! fraction of N retranslocated prior to leaf and root shedding
iftwolayersoil run_iftwolayersoil! 0		! Use the original LPJ-GUESS v4 soil scheme, or not. If true, override many of the switches below.
ifmultilayersnow run_ifmultilayersnow! 1 		! 0 is the old, single-layer scheme. 1 by default
ifinundationstress run_ifinundationstress! 1		! whether to reduce NPP if there's inundation (1), or not (0)
ifcarbonfreeze run_ifcarbonfreeze! 1		! Whether to limit soilC decomposition below 0 degC in upland soils (default, 1), or not (0)
wetland_runon run_wetland_runon! 0			! Extra daily water input or output (mm/day) to wetlands. Positive values are run ON, negative run OFF.
ifmethane run_ifmethane! 0			! Whether to run the methane model (for peatland only)
iforganicsoilproperties run_iforganicsoilproperties! 0	! Whether to update soil properties using soil carboon (for non peatland, mineral cells, soilcode != 8)
ifsaturatewetlands run_ifsaturatewetlands! 0		! Whether to take water from runoff to saturate low latitide wetlands (1), or not (0)
ifsmoothgreffmort run_ifsmoothgreffmort! 1				! whether to vary mort_greff smoothly with growth efficiency (1) 
ifdroughtlimitedestab run_ifdroughtlimitedestab! 1			! whether establishment is limited by growing season drought
ifrainonwetdaysonly run_ifrainonwetdaysonly! 1			! whether to rain on wet days only (1), or to rain a bit every day (0)
ifbvoc run_ifbvoc! 0              ! whether to include BVOC calculations (1) or not (0)
npatch run_npatch!_secondarystand run_npatch run_npatch!_secondarystand! 1		! number of replicate patches to simulate in secondary stands
reduce_all_stands run_reduce_all_stands! 0		! Whether to reduce equal percentage of all stands of a stand type at land cover change
age_limit_reduce run_age_limit_reduce! 15		! Minimum age of stands to reduce at land cover change
run_landcover run_run_landcover! 1        ! whether to use (and change) landcover fractions (1) or not (0)	
run_urban run_run_urban! 0	      ! whether to simulate barren land (1) or not (0)	
run_crop run_run_crop! 0	      ! whether to simulate crop vegetation (1) or not (0)
run_forest run_run_forest! 1	      ! whether to simulate managed forest (1) or not (0)
run_pasture run_run_pasture! 0	      ! whether to simulate pasture (1) or not (0)
run_natural run_run_natural! 1	      ! whether to simulate natural vegetation (1) or not (0)
run_peatland run_run_peatland! 0	      ! whether to simulate peatland (1) or not (0)
run_barren run_run_barren! 1	      ! whether to simulate barren land (1) or not (0)
ifslowharvestpool run_ifslowharvestpool! 1	! If a slow harvested product pool is included in patchpft.
gross_land_transfer run_gross_land_transfer! 0	! Whether to use gross land transfer: simulate gross lcc (1); read landcover transfer matrix input file (2); 
ifprimary_lc_transfer run_ifprimary_lc_transfer! 0	! Whether to use primary/secondary land transition info in landcover transfer input file (1). or not (0)
ifprimary_to_secondary_transfer run_ifprimary_to_secondary_transfer! 0 ! Whether to use primary-to-secondary land transition info (within land cover type) in landcover 
transfer_level run_transfer_level! 2	  ! Pooling level of land cover transitions; 0: one big pool; 1: land cover-level; 2: stand type-level
iftransfer_to_new_stand run_iftransfer_to_new_stand! 1 ! Whether to create new stands in transfer_to_new_stand() according to the rules in copy_stand_type(), if 0, soil C is not tracked after transfers, as they are pooled
suppress_disturbance_in_forestry_stands run_suppress_disturbance_in_forestry_stands! 0
minimizecftlist run_minimizecftlist! 1	   ! Checks st fraction file for st:s with fractions>0.0 at any time or place; other st:s are exluded from the stlist
frac_fixed_default_crops run_frac_fixed_default_crops! 0 ! whether to use all active crop stand types (0) or only stand types with suitable rainfed crops
nyears_cropland_ramp run_nyears_cropland_ramp! 0 	  ! Number of years to increase cropland fraction linearly from 0 to first year's value
ifintercropgrass run_ifintercropgrass! 1	  ! Whether to grow grass as cover crop between growing periods
iftillage run_iftillage! 1		  ! Whether to simulate tillage by increasing soil respiration
ifcalcdynamic_phu run_ifcalcdynamic_phu! 1	  ! Whether to calculate dynamic potential heat units
ifdyn_phu_limit run_ifdyn_phu_limit! 0	  ! Whether to limit dynamic phu calculation to a period specified by nyear_dyn_phu run_nyear_dyn_phu!
nyear_dyn_phu run_nyear_dyn_phu! 50	  ! Number of years to calculate dynamic phu if dynamic_phu_limit is 1
ifnlim run_ifnlim! 1
printseparatestands run_printseparatestands! 0
harvest_secondary_to_new_stand run_harvest_secondary_to_new_stand! 1
firsthistyear run_firsthistyear! 1600
lasthistyear run_lasthistyear! 2010
npatch run_npatch! 15			! number of replicate patches to simulate - 25
state_year run_state_year! 571			! year to save/start state file (no setting = after spinup) : saves the year before
restart run_restart! 0			! wheter to start from a state file
save_state run_save_state! 0			! wheter to save a state file
state_path run_state_path! "E:\Forest_structure\"		! directory to put state files in


group "common" (

	! Parameters common to all PFTs

	lambda_max common_lambda_max! 0.8
	emax common_emax! 5
	reprfrac common_reprfrac! 0.1
	wscal_min common_wscal_min! 0.35
	drought_tolerance common_drought_tolerance! 0.0001	
	! default value - ensures that this has little effect even if  
	! ifdroughtlimitedestab run_ifdroughtlimitedestab! is turned on
	landcover common_landcover! "natural"
	harv_eff common_harv_eff! 0
	res_outtake common_res_outtake! 0

	! Default peatland values unless overriden in PFT definitions
	wtp_max common_wtp_max! -301 		! [mm] maximum inundation (water table) tolerance, i.e. restrict on wetlands because wtp >= -300 always
	inund_duration common_inund_duration! 0	! [day] maximum days per month of inundation before GPP is reduced to 0
	min_snow common_min_snow! 0.0 		! [mm] minimum snow needed for establishment (DJF average) and survival (20-year average maximum),
				! i.e. No restriction here
	max_snow common_max_snow! 100000		! [mm] maximum snow tolerance (never used), i.e. No restriction here
	gdd0_max common_gdd0_max! 100000		! [degC day] max GDD0, i.e. No restriction here
	gdd0_min common_gdd0_min! 0		! [degC day] min GDD0, i.e. No restriction here
	has_aerenchyma common_has_aerenchyma! 0	! PFTs do not have aerenchyma for gas transport by default
)


group "tree" (

	! Parameters common to all trees

	common
	lifeform tree_lifeform! "tree"
	crownarea_max tree_crownarea_max! 40	
	turnover_root tree_turnover_root! 0.7
	ltor_max tree_ltor_max! 1
	k_allom2 tree_k_allom2! 60 ! CH: 40 in previous europe.ins but not specified in TH2012?; set to same as in global.ins; allows trees to grow higher to get a better fit for Vegetation C pool to inventory data 
		    ! 60 fits better to Widlowski et al. 2003 for FagSyl, PinSyl and in case of Pic abi better for diameter range > 15cm, whereas 40 (used by Hickler et al. 2012) fits better for Pic abi to <10cm
	k_allom3 tree_k_allom3! 0.67  ! Hickler et al. 2012: 0.67, Smith et al. 2001: 0.84, both use ref to Huang et al. 1992
	k_rp tree_k_rp! 1.6
	repr_age tree_repr_age! 30    ! CH: new introduced parameter to set age when reproduction begins
!	wooddens 200   ! CH: default is partitioned between needle and broad leaf, based on mean values for European species from the w    
	cton_root tree_cton_root! 29
	cton_sap tree_cton_sap! 330
	nuptoroot tree_nuptoroot! 0.0028
	km_volume tree_km_volume! 0.000001477
	pathway tree_pathway! "c3"
	kest_repr tree_kest_repr! 200
	kest_bg tree_kest_bg! 0.1
	kest_pres tree_kest_pres! 1
	k_chilla tree_k_chilla! 0
	k_chillb tree_k_chillb! 100
	k_chillk tree_k_chillk! 0.05
	litterme tree_litterme! 0.3
	rootdist 0.12 0.12 0.12 0.12 0.12 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04
	root_beta tree_root_beta! 0.982
	harv_eff tree_harv_eff! 0.9		! Value for forestry
	res_outtake tree_res_outtake! 0.4		! Value for forestry

	turnover_harv_prod tree_turnover_harv_prod! 0.04 ! Turnover rate for long-lived harvested products (wood) : 0.04 corresponds to a 25-year turnover time (as in Bondeau 2007)
	harvest_slow_frac tree_harvest_slow_frac! 0.33	! Bondeau 0.33
)


group "shrub" (
	common
	lifeform shrub_lifeform! "tree"
	leafphysiognomy shrub_leafphysiognomy! "broadleaf"
	crownarea_max shrub_crownarea_max! 10
	turnover_root shrub_turnover_root! 0.7
	ltor_max shrub_ltor_max! 1
	k_allom1 shrub_k_allom1! 100
	k_allom2 shrub_k_allom2! 5
	k_allom3 shrub_k_allom3! 0.67
	k_rp shrub_k_rp! 1.6
	wooddens shrub_wooddens! 200
	cton_root shrub_cton_root! 29
	cton_sap shrub_cton_sap! 330
	nuptoroot shrub_nuptoroot! 0.0028
	km_volume shrub_km_volume! 0.000001477
	fnstorage shrub_fnstorage! 0.3
	pathway shrub_pathway! "c3"
	kest_repr shrub_kest_repr! 200
	kest_bg shrub_kest_bg! 0.1
	kest_pres shrub_kest_pres! 1
	litterme shrub_litterme! 0.3
	rootdist 0.12 0.12 0.12 0.12 0.12 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04 0.04
	root_beta shrub_root_beta! 0.982

	! longevity shrub_longevity! 100 ! THreturned - in v3.1 this was set to 100 but here using no default longevity for shrubs	
	longevity shrub_longevity! 100

	harv_eff shrub_harv_eff! 0.9		! Value for forestry
	res_outtake shrub_res_outtake! 0.4		! Value for forestry
	turnover_harv_prod shrub_turnover_harv_prod! 1 ! Turnover rate for long-lived harvested products (wood) : 1-year turnover time
	harvest_slow_frac shrub_harvest_slow_frac! 0
)


group "needle_leaved" (
	leafphysiognomy needle_leaved_leafphysiognomy! "needleleaf"
